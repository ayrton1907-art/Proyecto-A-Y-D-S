<div class="container backgroundH1 col-12">
  <h3>Doumetos en los que estoy taggeados</h3>
</div>
<div class="row">
  <% @document.each do |element| %>
    <div class="col-xl-4">
      <div class="col-xl-10 elementColor">
        <a href="#"class="list-group-item list-group-item-action flex-column align-items-start  backgroundDoc">
          <embed src="<%=element.fileDocument%>#toolbar=0&navpanes=0&scrollbar=0" type="application/pdf" width="100%" height="100%" />
          <div class="d-flex w-100 justify-content-between">
            <h5 class="mb-1">Nombre: <%= element.name%></h5>
            <small class="text-muted"><%= element.date%></small>
          </div>
          <p class="mb-1"><%= element.description%></p>
        </a>
      </div>
    </div>
  <% end %>
</div>
