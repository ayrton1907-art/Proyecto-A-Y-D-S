           <% @collection.each do |element| %>
            <option  value = "<%= element.id%>" > <%= element.name%> </option>
           <% end %>
          </select>
        </div> -->
        <div class="form-group m-auto text-center">
          <button  id="buttonIndexA" type="submit" >Eliminar Suscripci√≥n</button>
        </div>
      </form>
    </div>
  <% end %>
</div>
