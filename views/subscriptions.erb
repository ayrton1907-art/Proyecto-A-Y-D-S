<div class="row">
  <%if @cat %>
        <div class="container col-xl-5">
          <form  class="container col-xl-11 backgroundFroms" method="POST" action="/add_subscriptions">
            <h4 class="fuenteUbuntu m-auto"> Categorias de Documentos </h4>
              <div class="col-xl-10 text-center">
                <select class="selectpicker show-menu-arrow backgroundImput" data-style="form-control" data-live-search="true" title="-- Selecciona las categorias --" multiple="multiple" name="nameSub[]">
                 <% @cat.each do |element| %>
                  <option  value = "<%= element.id%>" > <%= element.name%> </option>
                 <% end %>
                </select>
              </div>
              <div class="col-xl-10 text-center">
                <button id="buttonIndexA" type="submit" class="text-center">Suscribirme</button>
              </div>
          </form>
        </div>
  <% end %>
  <% if @collection %>
    <div class="container col-xl-5 ">
      <form class="container col-xl-11 backgroundFroms" method="POST" action="/delete_subscriptions">
        <div class="form-group col-4 text-center">
          <h4 class="fuenteUbuntu m-auto"> Subscripto </h4>
        </div>

        <div class="dropdown">
          <button onclick="myFunction()" class="dropbtn">Dropdown</button>
          <div id="myDropdown" class="dropdown-content">
            <input type="text" placeholder="Search.." id="myInput" onkeyup="filterFunction()">
            <a href="#about">About</a>
            <a href="#base">Base</a>
          </div>
        </div>






